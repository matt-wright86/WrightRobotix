<div class="page-header">
  <h1>WrightRobotix <small>(sign up)</small></h1>
</div>

<%= simple_form_for @user do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation, label: "Password Confirmation" %>
  <%= f.submit "Sign Up", class: "btn btn-primary" %>
<% end %>
